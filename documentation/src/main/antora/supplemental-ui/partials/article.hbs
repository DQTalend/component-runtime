<article class="doc">
    {{#if page.title}}
        <h1>{{{page.title}}}</h1>
    {{/if}}
    {{#if page.attributes.talend_contributors}}
        {{>contributors page}}
    {{else if page.attributes.talend_search}}
        {{>search}}
    {{else if page.attributes.popcorn_video}}
        {{>popcorn page}}
    {{else}}
        {{{page.contents}}}
    {{/if}}
</article>
