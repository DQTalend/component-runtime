{
  "v": "4.0",
  "workspace": {
    "projects": [
      {
        "name": "{{name}}",
        "attributes": {
          "languageVersion": [
            "1.8"
          ],
          "language": [
            "java"
          ]
        },
        "type": "maven",
        "source": {
          "location": "https://github.com/@organization@/@repository@.git",
          "type": "git",
          "parameters": {
            "keepVcs": "true",
            "branch": "master"
          }
        },
        "modules": [],
        "path": "/",
        "mixins": [
          "git"
        ],
        "problems": []
      }
    ],
    "defaultEnv": "wss",
    "name": "wss",
    "environments": {
      "wss": {
        "machines": {
          "dev-machine": {
            "agents": [
              "org.eclipse.che.terminal",
              "org.eclipse.che.ws-agent",
              "org.eclipse.che.ssh"
            ],
            "servers": {},
            "attributes": {
              "memoryLimitBytes": "2147483648"
            }
          }
        },
        "recipe": {
          "location": "codenvy/ubuntu_jdk8",
          "type": "dockerimage"
        }
      }
    }
  },
  "name": "{{name}}",
  "creator": {
    "name": "{{company}}",
    "email": "github@{{company}}.com"
  }
}
